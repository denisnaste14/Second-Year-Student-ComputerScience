<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="479.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ShowFriendRequestController">
   <children>
      <Label layoutX="99.0" layoutY="52.0" text="All of my friend requests: " />
      <TableView fx:id="table" layoutX="99.0" layoutY="90.0" onMouseClicked="#check_update" prefHeight="304.0" prefWidth="504.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="241.0" text="Name" />
          <TableColumn fx:id="status" prefWidth="262.0" text="Status" />
        </columns>
      </TableView>
      <Button layoutX="123.0" layoutY="425.0" mnemonicParsing="false" onMouseClicked="#accept_btn" prefHeight="25.0" prefWidth="112.0" text="Accept" />
      <Button layoutX="468.0" layoutY="425.0" mnemonicParsing="false" onMouseClicked="#reject_btn" prefHeight="25.0" prefWidth="112.0" text="Reject" />

      

   </children>
</AnchorPane>
